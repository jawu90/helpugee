		"@types/leaflet": "^1.7.10",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"broadcast-channel": "^4.13.0",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
